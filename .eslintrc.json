{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "func-names": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-mixed-operators": 0,
    "no-plusplus": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "vars-on-top": 0
  }
}
